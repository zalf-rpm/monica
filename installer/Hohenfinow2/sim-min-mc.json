{
	"crop.json": "crop-min-mc.json",
	"site.json": "site-min.json",
	"climate.csv": "climate-min.csv",

	"climate.csv-options": {
		"no-of-climate-file-header-lines": 2,
		"csv-separator": ","
	},
	
	"debug?": false,
	"include-file-base-path": "${MONICA_PARAMETERS}/",

	"output": { 
	  	"write-file?": true,

		"path-to-output": "./",
		"file-name": "sim-min-out.csv",
	
		"csv-options": {
			"include-header-row": true,
			"include-units-row": true,
			"include-aggregation-rows": false,
			"csv-separator": ","
		},

		"events" : [
			"daily", [
				"Date",
				["Crop|Crop1", {"cropId": "WW1"}],
				["Stage|Stage1", {"cropId": "WW1"}],
				["AbBiom|AbBiom1", {"cropId": "WW1"}],
				["Yield|Yield1", {"cropId": "WW1"}],
				["Evapotranspiration|Evapotranspiration1", {"cropId": "WW1"}],
				["Evaporation|Evaporation1", {"cropId": "WW1"}],
				["Tra|Tra1", {"cropId": "WW1"}],
				["Act_ET|Act_ET1", {"cropId": "WW1"}],
				["ETa/ETc|ETa/ETc1", {"cropId": "WW1"}],
				["Crop|Crop2", {"cropId": "WW2"}],
				["Stage|Stage2", {"cropId": "WW2"}],
				["AbBiom|AbBiom2", {"cropId": "WW2"}],
				["Yield|Yield2", {"cropId": "WW2"}],
				["Evapotranspiration|Evapotranspiration2", {"cropId": "WW2"}],
				["Evaporation|Evaporation2", {"cropId": "WW2"}],
				["Tra|Tra2", {"cropId": "WW2"}],
				["Act_ET|Act_ET2", {"cropId": "WW2"}],
				["ETa/ETc|ETa/ETc2", {"cropId": "WW2"}],
				["Mois", [1, 6]],
				["N", [1, 6]],
				["rootDensity",[1,6]]
			],

			"crop|WW1", [
				"CM-count",
				["Crop", "FIRST", {"cropId": "WW1"}],
				["Yield", "LAST", {"cropId": "WW1"}],
				["Date|sowing", "FIRST"],
				["Date|harvest", "LAST"]
			],

			"crop|WW2", [
				"CM-count",
				["Crop", "FIRST", {"cropId": "WW1"}],
				["Yield", "LAST", {"cropId": "WW2"}],
				["Date|sowing", "FIRST"],
				["Date|harvest", "LAST"]
			],

			"yearly", [
				"Year", 
				["N", [1, 3, "AVG"], "SUM"],
				["RunOff", "SUM"], 
				["NLeach", "SUM"], 
				["Recharge", "SUM"]
			],

			"run", [
				["Precip", "SUM"]
			]
		]
	},

	"UseSecondaryYields": true,
	"NitrogenResponseOn": true,
	"WaterDeficitResponseOn": true,
	"EmergenceMoistureControlOn": false,
	"EmergenceFloodingControlOn": false,

	"UseNMinMineralFertilisingMethod": false,
	"NMinUserParams": { "min": 40, "max": 120, "delayInDays": 10 },
	"NMinFertiliserPartition": ["include-from-file", "mineral-fertilisers/AN.json"],
	"JulianDayAutomaticFertilising": 89,

	"UseAutomaticIrrigation": false,
	"AutoIrrigationParams": {
		"irrigationParameters": {
			"nitrateConcentration": [0, "mg dm-3"]
		},
		"startDate": "1992-05-01",
		"amount": [17, "mm"],
		"trigger_if_nFC_below_%": [90, "%"],
		"_set_to_%nFC": [100, "%"],
		"calc_nFC_until_depth_m": [0.3, "m"]
	},

	"serializedMonicaState": {
		"load": {
			"atStart": false,
			"fromJson": true,
			"path": "/home/berg/GitHub/monica/test-ser.json"
		},
		"save": {
			"atEnd": false,
			"toJson": true,
			"path": "/home/berg/GitHub/monica/test-ser.json",
			"noOfPreviousDaysSerializedClimateData": 10
		}
	}
}
